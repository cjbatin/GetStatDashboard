<!DOCTYPE html>
<%- include('navbar'); -%>
<body>
    <div class="container">
    <% if (data.results.length > 0 ){%>
        <% if (data.numberOfPages > 0 ){%>
            <ul class="pagination center">
                <% if (data.pageNumber === 1) { %>
                    <li class="disabled"><a href=<%="/" + data.apiKey + "/site/" + data.siteId + "/tags?page=1"%>><i class="material-icons">chevron_left</i></a></li>
                <%} else { %>
                    <li class="waves-effect"><a href=<%="/" + data.apiKey + "/site/" + data.siteId + "/tags?page=1"%>><i class="material-icons">chevron_left</i></a></li>
                <% } %>
                <%  for (var i = 1; i <= data.numberOfPages; i++) { %>
                    <% if (data.pageNumber === i) { %>
                        <li class="active"><a href=<%="/" + data.apiKey + "/site/" + data.siteId + "/tags?page=" + i %>><%= i %></a></li>
                    <%} else { %>
                        <li class="waves-effect"><a href=<%="/" + data.apiKey + "/site/" + data.siteId + "/tags?page=" + i %>><%= i %></a></li>
                    <% } %>
                <% } %>
                <% if (data.pageNumber === data.numberOfPages) { %>
                    <li class="disabled"><a href=<%="/" + data.apiKey + "/site/" + data.siteId + "/tags?page=" + data.numberOfPages %>><i class="material-icons">chevron_right</i></a></li>
                <%} else { %>
                    <li class="waves-effect"><a href=<%="/" + data.apiKey + "/site/" + data.siteId + "/tags?page=" + data.numberOfPages %>><i class="material-icons">chevron_right</i></a></li>
                <% } %>
            </ul>
        <%}%>
        <%  for (let result of data.results){ %>
            <div class="row">
                <div class="col s12 m4 l2"></div>
                    <div class="col s12 m4 l8">
                        <div class="card blue-grey darken-1">
                        <div class="card-content white-text">
                            <span class="card-title"><%= result.Tag %> </span>
                            <p>Id: <%= result.Id %></p>
                            <p>Type: <%= result.Type %></p>
                        </div>
                        <div class="card-action">
                            <a href=<%="/" + data.apiKey + "/site/" + result.Id + "/tags" %>>See Keywords</a>
                        </div>
                        </div>
                    </div>
                <div class="col s12 m4 l2"></div>
            </div>
        <% } %>
    <%}%>
    </div>
</body>
</html>